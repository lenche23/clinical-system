<Window x:Class="vezba.WindowAddRoomEquipment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vezba"
        mc:Ignorable="d"
        Title="WindowAddRoomEquipment" Height="497.083" Width="429.879" Background="#FFFFC1A4">
    <Grid Margin="0,0,-8,-6">
        <Label Name="Title" Content="Dodavanje nove opreme u prostoriju"  FontSize="18" FontFamily="Myriad Pro Cond" Margin="40,35,30,370"/>
        <Label Content="Naziv opreme:" Margin="84,96,246,347"/>
        <Label Content="ItemQuantity:" Margin="115,152,246,296"/>
        <TextBox Name="Količina" Height ="24" RenderTransformOrigin="0.491,0.484" Margin="184,146,118,290" />
        <Button Click="PotvrdiDodavanje_Button_Click" Margin="65,215,231,212">Potvrdi</Button>
        <Button Click="OdustaniDodavanje_Button_Click" Margin="235,215,55,212">Odustani</Button>
        <ComboBox ItemsSource="{Binding equipmentList}" DisplayMemberPath="Name" Name="comboEquipmentName" Margin="184,96,118,347"></ComboBox>
    </Grid>
</Window>
