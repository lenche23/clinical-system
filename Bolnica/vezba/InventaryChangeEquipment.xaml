<Window x:Class="vezba.InventaryChangeEquipment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vezba"
        mc:Ignorable="d"
        Title="InventaryChangeEquipment" Height="497.083" Width="429.879" Background="#FFFFC1A4">
    <Grid>
        <Label Content="Izmena opreme "  FontSize="18" FontFamily="Myriad Pro Cond" Margin="155,38,117,381" RenderTransformOrigin="0.643,0.468"/>
        <Label Content="Naziv opreme:" Margin="93,124,206,260"/>
        <Label Content="Tip opreme:" Margin="104,194,206,186" />
        <TextBox Name="NazivOpreme" Text="{Binding Name, Mode=OneWay}" Margin="190,124,95,305" RenderTransformOrigin="0.491,0.484" />
        <Button Click="Okay_Button_Click" Margin="64,290,227,130">Potvrdi</Button>
        <Button Click="Cancel_Change_Button_Click" Margin="224,290,61,130">Odustani</Button>

        <RadioButton Name ="Statička" GroupName="TipOpreme" Content="Statička" Margin="186,200,167,232"></RadioButton>
        <RadioButton Name ="Dinamička" GroupName="TipOpreme" Content="Dinamička" Margin="260,200,61,232"></RadioButton>
    </Grid>
</Window>
