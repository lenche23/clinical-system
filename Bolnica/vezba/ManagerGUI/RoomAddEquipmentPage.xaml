<Page x:Class="vezba.ManagerGUI.RoomAddEquipmentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:vezba.ManagerGUI"
      mc:Ignorable="d" 
      d:DesignHeight="570" d:DesignWidth="750"
      Title="RoomAddEquipmentPag" Background="#FFFFC1A4">
    <Grid Margin="0,0,-8,-6">
        <Label Name="Title" Content="Dodavanje nove opreme u prostoriju"  FontSize="18" FontFamily="Myriad Pro Cond" Margin="40,35,30,370"/>
        <Label Content="Naziv opreme:" Margin="84,96,579,446"/>
        <Label Content="ItemQuantity:" Margin="115,152,543,400"/>
        <TextBox Name="Količina" Height ="24" RenderTransformOrigin="0.491,0.484" Margin="220,152,458,400" />
        <Button Click="PotvrdiDodavanje_Button_Click" Margin="65,305,433,212">Potvrdi</Button>
        <Button Click="OdustaniDodavanje_Button_Click" Margin="405,305,55,212">Odustani</Button>
        <ComboBox ItemsSource="{Binding equipmentList}" DisplayMemberPath="Name" Name="comboEquipmentName" Margin="184,96,313,446"></ComboBox>
    </Grid>
</Page>
